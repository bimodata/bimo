{
  "name": "@bimo/core-utils-async-for-each",
  "version": "0.0.1",
  "description": "Async loops over arrays, maps, objects",
  "main": "dist/cjs/index.js",
  "author": "Gaël Haméon <gael.hameon@gmail.com>",
  "license": "LGPL-3.0-or-later",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./types",
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "build",
    "test": "mocha",
    "coverage": "nyc --reporter=html --reporter=text npm test",
    "prepublish": "tsc",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.9",
    "chai": "^4.5.0",
    "mocha": "^11.1.0",
    "nyc": "^17.1.0",
    "rimraf": "^6.0.1",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.0.2"
  },
  "homepage": "https://bimodata.com/",
  "bugs": "https://github.com/bimodata/bimo/issues",
  "repository": "github:bimodata/bimo",
  "types": "types/cjs/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./types/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./types/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  }
}
